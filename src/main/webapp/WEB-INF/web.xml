<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>net.jaeger.Servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>createUser</servlet-name>
    <servlet-class>net.jaeger.Servlet.CreateUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>createUser</servlet-name>
    <url-pattern>/User/CreateUserServlet</url-pattern>
  </servlet-mapping>
</web-app>
